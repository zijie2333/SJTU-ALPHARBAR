<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:fancy="http://schemas.android.com/apk/res-auto"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    android:background="@drawable/main111">

    <yalantis.com.sidemenu.sample.FancyButton
        android:id="@+id/myguide"
        android:layout_width="wrap_content"
        android:layout_height="38dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="30dp"
        android:paddingBottom="10dp"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:paddingTop="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        fancy:fb_borderColor="#e2e067"
        fancy:fb_borderWidth="1dp"
        fancy:fb_defaultColor="#edede8"
        fancy:fb_focusColor="#f15aa8"
        fancy:fb_iconPosition="left"
        fancy:fb_radius="30dp"
        fancy:fb_text="Drink MENU"
        fancy:fb_textColor="#2d2c1c" />

        <ListView
            android:id="@+id/list_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="100dp" />


</android.support.constraint.ConstraintLayout>



